{
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noImplicitReturns": true,
        "lib": ["dom", "esnext"],
        "allowJs": true,
        "jsx": "react-jsx",
        "strict": true,
        "resolveJsonModule": true
    },
    "include": ["src"]
}